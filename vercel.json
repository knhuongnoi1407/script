{
  "rewrites": [
    {
      "source": "/get",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*PowerShell.*"
        }
      ],
      "destination": "/kn-script.ps1"
    },
    {
      "source": "/get",
      "destination": "/browser-block.html"
    }
  ],
  "headers": [
    {
      "source": "/kn-script.ps1",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        }
      ]
    }
  ]
}
