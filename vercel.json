{
  "rewrites": [
    {
      "source": "/get",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*PowerShell.*"
        }
      ],
      "destination": "/kn-script.ps1"
    },
    {
      "source": "/get",
      "destination": "/browser-block.html"
    }
  ]
}
