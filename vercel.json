{
  "rewrites": [
    { "source": "/get", "destination": "/kn-script.ps1" }
  ],
  "headers": [
    {
      "source": "/get",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*PowerShell.*"
        }
      ],
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        }
      ]
    },
    {
      "source": "/get",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}
